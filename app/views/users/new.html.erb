<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for(@user) do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>
   <br>
   <%= f.label :height %>
   <%= f.text_field :height %>
   <br>
   <%= f.label :happiness %>
   <%= f.text_field :happiness %>
   <br>
   <%= f.label :nausea %>
   <%= f.text_field :nausea %>
   <br>
   <%= f.label :tickets %>
   <%= f.text_field :tickets %>
   <br>
   <%= f.label :password %>
   <%= f.password_field :password %>
   <br>
   <%= f.submit 'Create User'%>
<% end %>